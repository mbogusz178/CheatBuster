<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.Scene?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.ButtonBar?>
<?import javafx.scene.control.ListView?>
<?import javafx.scene.control.ScrollPane?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.VBox?>
<?import javafx.stage.Stage?>

<Stage fx:id="stage" minHeight="500.0" minWidth="400.0" title="Opcje CheatBuster" xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="schumi178.javaprograms.cheatbuster.gui.OptionsScreen">
    <scene>
        <Scene>
         <VBox fx:id="vBox" minWidth="400.0" prefWidth="597.0">
            <children>
               <HBox fx:id="hBox" minWidth="400.0" prefHeight="437.0" prefWidth="597.0">
                  <children>
                     <ListView fx:id="categoryList" maxWidth="200.0" minWidth="200.0" prefHeight="200.0" prefWidth="200.0" />
                     <ScrollPane fx:id="optionsContent" prefHeight="200.0" prefWidth="200.0" HBox.hgrow="ALWAYS" />
                  </children>
               </HBox>
               <ButtonBar prefHeight="40.0" prefWidth="200.0">
                 <buttons>
                     <Button mnemonicParsing="false" onAction="#onOKClick" text="OK" />
                     <Button mnemonicParsing="false" onAction="#onCancelClick" text="Anuluj" />
                   <Button mnemonicParsing="false" onAction="#onApplyClick" text="Zastosuj" />
                 </buttons>
                  <VBox.margin>
                     <Insets />
                  </VBox.margin>
                  <padding>
                     <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
                  </padding>
               </ButtonBar>
            </children>
         </VBox>
        </Scene>
    </scene>
</Stage>
